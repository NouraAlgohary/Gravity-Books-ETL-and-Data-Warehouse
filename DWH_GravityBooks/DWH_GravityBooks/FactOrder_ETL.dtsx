                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
 co.order_id AS order_id_BK,
 co.order_date,
  ol.line_id As line_id_BK,
  bo.book_id AS book_id_BK,
  co.customer_id AS Customer_id,
